<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
     <item android:state_checked="true" android:id="@+id/iconBubble">
        <shape android:shape="oval">
            <solid android:color="@color/icon_back" />
            <size
                android:height="50dp"
                android:width="50dp"/>
            <stroke
                android:color="@color/icon_back"
                android:width="2dp"
                />
        </shape>
    </item>
    <item
        android:drawable="@drawable/ic_lauch_round"
        android:width="45dp"
        android:height="45dp"

        android:gravity="center">
    </item>

</layer-list>
